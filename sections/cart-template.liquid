<div>
  <h1>Cart Template</h1>

  {% for items in cart.items %}
    <img src="{{ items.image | image_url: width: 200 }}" width="100" height="100">
    <h1>{{ items.product.title }} </h1>
    <h1>{{ items.variant.title }} </h1>

    <a class="text-red-500"  href="{{ items.url_to_remove }}"> Remove </a>
    <p>{{ items.product.price |money }} </p>
  {% endfor %}
</div>

{% schema %}
{
  "name": "Cart Template",
  "settings": []
}
{% endschema %}
